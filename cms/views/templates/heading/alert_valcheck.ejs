<% try { %>
	<% show_alert = show_alert %>
<% } catch(err) { %>
	<% show_alert = false; %>
<% } %>

<% if(show_alert != true && show_alert != false) { show_alert = false; } %>

<% try { %>
	<% alert_path = alert_path %>
<% } catch(err) { %>
	<% alert_path = ''; %>
<% } %>

<% try { %>
	<% message = message + ''; %>
<% } catch(err) { %>
	<% message = '' %>
<% } %>